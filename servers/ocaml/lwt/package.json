{
  "name": "ocaml-graphql-server",
  "version": "0.1.0",
  "description": "Example Reason Esy Project",
  "license": "MIT",
  "esy": {
    "build": "refmterr dune build",
    "buildsInSource": "_build",
    "install": [ "esy-installer #{self.name}.install" ],
